	cmake ${srcdir}/kde-l10n-el-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/en_GB
	cd build/en_GB
