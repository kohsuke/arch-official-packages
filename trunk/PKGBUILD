	mkdir -p build/uk
	cd build/uk
	[ -d ${srcdir}/kde-l10n-uk-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-uk-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-uk-${pkgver}/messages/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-uk-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-uk-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/wa
	cd build/wa
	[ -d ${srcdir}/kde-l10n-wa-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-wa-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-wa-${pkgver}/messages/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-wa-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-wa-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/zh_CN
	cd build/zh_CN
	[ -d ${srcdir}/kde-l10n-zh_CN-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-zh_CN-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-zh_CN-${pkgver}/messages/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-zh_CN-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-zh_CN-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/zh_TW
	cd build/zh_TW
	[ -d ${srcdir}/kde-l10n-zh_TW-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-zh_TW-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-zh_TW-${pkgver}/messages/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-zh_TW-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-zh_TW-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

}

