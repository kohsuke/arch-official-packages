	mkdir -p build/ga
	cd build/ga
	[ -d ${srcdir}/kde-l10n-ga-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-ga-${pkgver}/docs/CMakeLists.txt
