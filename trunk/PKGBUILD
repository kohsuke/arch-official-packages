build() {
  for i in ${pkgname[@]}
    do _pkglang=${i#*-}
     echo ${_pkglang} " unpacking..."
     cd ${srcdir}/LibO_${_oover/-/}_Linux_x86_langpack-rpm_${_pkglang}/RPMS
     # remove dictionaries if shipped - we ship them now in separate packages
     rm libreoffice3-dict-*.rpm || /bin/true
	for j in *.rpm
	 do rpmextract.sh $j
	done
     # the helppack rpm goes also into the langpack dir - so far we don't want to split the helppacks separate
#     rpmextract.sh ${srcdir}/LibO_${_oover/-/}_Linux_x86_helppack-rpm_${_pkglang}/RPMS/libobasis3.3-${_pkglang}-help-${_helpver}.i586.rpm
     rpmextract.sh ${srcdir}/LibO_${_oover/-/}_Linux_x86_helppack-rpm_${_pkglang}/RPMS/*.rpm
  done
}

package_libreoffice-af() {
  pkgdesc="Afrikaans language pack for LibreOffice"
  cd ${srcdir}/LibO_${_oover/-/}_Linux_x86_langpack-rpm_af/RPMS

  # install libreoffice language files
  mkdir -p ${pkgdir}/usr/lib/ooo-${_oomainver}/basis-link
  cp -R opt/libreoffice/basis${_oomainver}/* ${pkgdir}/usr/lib/ooo-${_oomainver}/basis-link
  cp -R opt/libreoffice/{program,readmes} ${pkgdir}/usr/lib/ooo-${_oomainver}
  chown root:root -R ${pkgdir}/usr/lib/ooo-${_oomainver}
}

