pkgname=python-gdata
pkgver=1.3.0
pkgrel=1
arch=('i686' 'x86_64')
pkgdesc="Python client library for Google data APIs"
url="http://code.google.com/p/gdata-python-client/"
license=('APACHE')
depends=('python>=2.5.2')
provides=('gdata_py')
replaces=('gdata_py')
conflicts=('gdata_py')
source=(http://gdata-python-client.googlecode.com/files/gdata-${pkgver}.tar.gz)

build() {
  cd ${startdir}/src/gdata-${pkgver}
  python setup.py install --prefix=/usr --root ${startdir}/pkg || return 1
}
md5sums=('77f84403f1d3bac6f48f634e46cb4d76')
