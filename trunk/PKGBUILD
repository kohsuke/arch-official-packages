	cd build/fr
	[ -d ${srcdir}/kde-l10n-fr-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-fr-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-fr-${pkgver}/messages/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-fr-${pkgver}/messages/CMakeLists.txt
