	cmake ${srcdir}/kde-l10n-cs-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/da
	cd build/da
	[ -d ${srcdir}/kde-l10n-da-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-da-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-da-${pkgver}/messages/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-da-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-da-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/de
	cd build/de
	[ -d ${srcdir}/kde-l10n-de-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-de-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-de-${pkgver}/messages/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-de-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-de-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/el
	cd build/el
	[ -d ${srcdir}/kde-l10n-el-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-el-${pkgver}/docs/CMakeLists.txt
