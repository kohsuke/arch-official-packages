build() {
  cd ${srcdir}

  for lang in ar bg bs ca ca@valencia cs da de el en_GB es et eu fa fi fr ga gl he hr hu ia id is it ja kk km ko lt lv nb nds nl nn pa pl pt pt_BR ro ru si sk sl sr sv tg th tr ug uk vi wa zh_CN zh_TW; do
    mkdir -p build/${lang}
    cd build/${lang}
    cmake ${srcdir}/${pkgbase}-${lang}-${pkgver} \
      -DCMAKE_BUILD_TYPE=Release \
      -DCMAKE_INSTALL_PREFIX=/usr
    make
    cd ../..
  done
}

package_kde-l10n-ar() {
	pkgdesc=('Arabic Localization for KDE')
	cd ${srcdir}/build/ar
	make DESTDIR=${pkgdir} install
}

