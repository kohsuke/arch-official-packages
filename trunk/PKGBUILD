  cd ${srcdir}/kid/RPMS

  # install libreoffice language files
  mkdir -p ${pkgdir}/usr/lib/ooo-${_oomainver}/basis-link
  cp -R opt/libreoffice3/basis${_oomainver}/* ${pkgdir}/usr/lib/ooo-${_oomainver}/basis-link
  cp -R opt/libreoffice3/{licenses,program,readmes,share} ${pkgdir}/usr/lib/ooo-${_oomainver}
  chown root:root -R ${pkgdir}/usr/lib/ooo-${_oomainver}
}

