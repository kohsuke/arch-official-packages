package_kde-l10n-ar() {
	pkgdesc=('Arabic Localization for KDE')
	replaces=('kde-i18n-ar')
	provides=('kde-i18n-ar')
	cd $srcdir/build/ar
	make DESTDIR=$pkgdir install
}

package_kde-l10n-bg() {
	pkgdesc=('Bulgarian Localization for KDE')
	replaces=('kde-i18n-bg')
	provides=('kde-i18n-bg')
	cd $srcdir/build/bg
	make DESTDIR=$pkgdir install
}

