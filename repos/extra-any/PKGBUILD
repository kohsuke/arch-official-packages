build() {
    cd ${srcdir}/kde-l10n-nl-${pkgver}
    patch -p4 -i ${srcdir}/kde-l10n-nl-fix.patch

	cd ${srcdir}
	mkdir -p build/ar
	cd build/ar
	[ -d ${srcdir}/kde-l10n-ar-${pkgver}/docs/kdepim ] && echo 'add_subdirectory( kdepim )' >> ${srcdir}/kde-l10n-ar-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-ar-${pkgver}/messages/kdepim ] && echo 'add_subdirectory( kdepim )' >> ${srcdir}/kde-l10n-ar-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-ar-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/bg
	cd build/bg
	[ -d ${srcdir}/kde-l10n-bg-${pkgver}/docs/kdepim ] && echo 'add_subdirectory( kdepim )' >> ${srcdir}/kde-l10n-bg-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-bg-${pkgver}/messages/kdepim ] && echo 'add_subdirectory( kdepim )' >> ${srcdir}/kde-l10n-bg-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-bg-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/ca
	cd build/ca
	[ -d ${srcdir}/kde-l10n-ca-${pkgver}/docs/kdepim ] && echo 'add_subdirectory( kdepim )' >> ${srcdir}/kde-l10n-ca-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-ca-${pkgver}/messages/kdepim ] && echo 'add_subdirectory( kdepim )' >> ${srcdir}/kde-l10n-ca-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-ca-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/ca@valencia
	cd build/ca@valencia
	[ -d ${srcdir}/kde-l10n-ca@valencia-${pkgver}/docs/kdepim ] && echo 'add_subdirectory( kdepim )' >> ${srcdir}/kde-l10n-ca@valencia-${pkgver}/docs/CMakeLists.txt
