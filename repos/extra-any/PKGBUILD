build() {
	cd ${srcdir}
	mkdir -p build/ar
	cd build/ar
	[ -d ${srcdir}/kde-l10n-ar-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-ar-${pkgver}/docs/CMakeLists.txt
	[ -d ${srcdir}/kde-l10n-ar-${pkgver}/messages/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-ar-${pkgver}/messages/CMakeLists.txt
	cmake ${srcdir}/kde-l10n-ar-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/bg
	cd build/bg
