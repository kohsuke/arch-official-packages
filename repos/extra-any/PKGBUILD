build() {
	cd ${srcdir}
	mkdir -p build/ar
	cd build/ar
	cmake ${srcdir}/kde-l10n-ar-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/bg
	cd build/bg
	cmake ${srcdir}/kde-l10n-bg-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/bs
	cd build/bs
	cmake ${srcdir}/kde-l10n-bs-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
