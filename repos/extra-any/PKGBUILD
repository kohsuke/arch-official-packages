         '9b85ed18c8b24ec73043e5207590e975'
         'a55f464ea08baf262e14a768eae8aec2'
         '8b247846a07f4a6013f09126b11f47a5'
         '54781af7f247b80d518e61421c9f557a'
         '2156eaed110d86033330aa023b2dadb2'
         '6b8482a457ff7f5b5e23362a3fb89cba')

build() {
  for i in ${pkgname[@]}
    do _pkglang=${i#*-}
     echo ${_pkglang} " unpacking..."
#     cd ${srcdir}/${_pkglang}/RPMS
     cd ${srcdir}/LibO_${_oover/-/}_${_rcdate}_Linux_x86_langpack-rpm_${_pkglang}/RPMS
	for j in *.rpm
	 do rpmextract.sh $j
	done
  done
}

package_libreoffice-af() {
  pkgdesc="Afrikaans language pack for LibreOffice"
  cd ${srcdir}/LibO_${_oover/-/}_${_rcdate}_Linux_x86_langpack-rpm_af/RPMS

  # install libreoffice language files
  mkdir -p ${pkgdir}/usr/lib/ooo-${_oomainver}/basis-link
  cp -R opt/libreoffice/basis${_oomainver}/* ${pkgdir}/usr/lib/ooo-${_oomainver}/basis-link
  cp -R opt/libreoffice/{licenses,program,readmes,share} ${pkgdir}/usr/lib/ooo-${_oomainver}
  chown root:root -R ${pkgdir}/usr/lib/ooo-${_oomainver}
}

