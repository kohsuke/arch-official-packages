	cmake ${srcdir}/kde-l10n-de-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/el
	cd build/el
	[ -d ${srcdir}/kde-l10n-el-${pkgver}/docs/kdepim ] && sed -i '2i add_subdirectory( kdepim )' ${srcdir}/kde-l10n-el-${pkgver}/docs/CMakeLists.txt
