	cd ../..

	mkdir -p build/tr
	cd build/tr
	cmake ${srcdir}/kde-l10n-tr-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/ug
	cd build/ug
	cmake ${srcdir}/kde-l10n-ug-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/uk
	cd build/uk
	cmake ${srcdir}/kde-l10n-uk-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/wa
	cd build/wa
	cmake ${srcdir}/kde-l10n-wa-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/zh_CN
	cd build/zh_CN
	cmake ${srcdir}/kde-l10n-zh_CN-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/zh_TW
	cd build/zh_TW
	cmake ${srcdir}/kde-l10n-zh_TW-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

}

